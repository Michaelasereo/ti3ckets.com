{
  "name": "@getiickets/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prisma:generate": "prisma generate --schema=../../packages/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=../../packages/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=../../packages/database/prisma/schema.prisma",
    "seed": "tsx src/scripts/seed.ts",
    "check-indexes": "tsx src/scripts/check-indexes.ts",
    "test-db-connection": "tsx src/scripts/test-db-connection.ts",
    "send-test-ticket-email": "tsx src/scripts/send-test-ticket-email.ts"
  },
  "dependencies": {
    "@getiickets/database": "*",
    "@getiickets/shared": "*",
    "@getiickets/config": "*",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.14.0",
    "qrcode": "^1.5.3",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "@aws-sdk/client-s3": "^3.490.0",
    "zod": "^3.22.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/multer": "^1.4.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/pdfkit": "^0.13.0",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
